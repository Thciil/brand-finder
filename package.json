{
  "name": "sponsor-pathfinder",
  "version": "1.0.0",
  "description": "Discover, qualify, and route sponsorship leads for the Panna World Championship",
  "main": "dist/index.js",
  "bin": {
    "sponsor-pathfinder": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "cli": "ts-node src/index.ts",
