    "@vercel/postgres": "^0.10.0",
    "better-sqlite3": "^12.6.2",
    "cheerio": "^1.2.0",
    "commander": "^14.0.2",
    "dotenv": "^17.2.3",
    "next": "^16.1.6",
    "openai": "^6.17.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
